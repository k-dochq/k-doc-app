{
  "expo": {
    "name": "K-DOC",
    "slug": "k-doc-app",
    "version": "1.5.001",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFDBF9",
          "image": "./assets/splash.png",
          "android": {
            "image": "./assets/splash.png",
            "resizeMode": "contain"
          },
          "ios": {
            "image": "./assets/splash.png",
            "resizeMode": "cover"
          }
        }
      ],
      "./plugins/withKeystoreConfig",
      "./plugins/withIOSVersion",
      "./plugins/withAndroidCleartextTraffic",
      "./plugins/withIOSTeam",
      "expo-asset"
    ],
    "scheme": "kdoc",
    "ios": {
      "bundleIdentifier": "io.drmiracle.app",
      "buildNumber": "1.5.001",
      "version": "1.5.001",
      "supportsTablet": true,
      "teamId": "3LC3JLW6J5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "io.drmiracle.app",
      "versionCode": 15,
      "versionName": "1.5.001",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#ffffff",
        "iconShape": "square"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e8d1a981-a5cf-46d7-a157-c3dc719f12bc"
      }
    }
  }
}
