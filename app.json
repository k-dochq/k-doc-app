{
  "expo": {
    "name": "K-DOC",
    "slug": "k-doc-app",
    "version": "1.5.6104",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#DA47EF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#DA47EF",
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "android": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#DA47EF"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "k-doc"
        }
      ],
      "./plugins/withKeystoreConfig",
      "./plugins/withIOSVersion",
      "./plugins/withAndroidCleartextTraffic",
      "./plugins/withIOSTeam",
      "expo-asset",
      "expo-secure-store",
      "react-native-appsflyer",
      "expo-tracking-transparency",
      "expo-web-browser"
    ],
    "scheme": "kdoc",
    "ios": {
      "bundleIdentifier": "io.drmiracle.app",
      "buildNumber": "63",
      "version": "1.5.6104",
      "supportsTablet": true,
      "teamId": "3LC3JLW6J5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "맞춤형 마케팅과 앱 성능 분석을 위해 기기 식별자를 사용합니다."
      }
    },
    "android": {
      "package": "io.drmiracle.app",
      "versionCode": 61,
      "versionName": "1.5.6104",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#DA47EF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e8d1a981-a5cf-46d7-a157-c3dc719f12bc"
      },
      "appsFlyer": {
        "devKey": "tjf3bfias5hyxjZ8ouBoYi",
