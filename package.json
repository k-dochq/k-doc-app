{
  "name": "k-doc-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios --device \"iPhone\"",
    "web": "expo start --web",
    "clean": "rm -rf android ios",
    "prebuild": "expo prebuild",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:ios": "expo prebuild --platform ios",
    "rebuild:android": "rm -rf android && npm run prebuild:android && npm run android",
    "rebuild:ios": "npm run prebuild:ios && npm run ios",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "generate-apple-secret": "tsx scripts/generate-apple-secret.ts"
  },
  "dependencies": {
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.75.0",
    "dotenv": "^17.2.3",
    "escape-string-regexp": "^5.0.0",
    "expo": "~54.0.10",
    "expo-asset": "~12.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-av": "~16.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "^6.0.15",
    "expo-device": "~8.0.9",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-tracking-transparency": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "jose": "^6.1.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-appsflyer": "^6.17.5",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-webview": "^13.15.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
